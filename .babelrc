{
  "env": {
    "development": {
      "plugins": [
        [
          "styled-components",
          { "ssr": true, "displayName": true }
        ]
      ],
      "presets": [ "next/babel" ]
    },
    "test": {
      "plugins": [ "styled-jsx/babel" ], // for<style jsx>
      "presets": [ "@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript" ]
    }
  }
}